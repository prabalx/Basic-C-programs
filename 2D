#include<stdio.h>
int main ()
{
	int i,j, x[3][3];
	for (i=0; i<3; i++)
	{
	
		for (j=0;j<3;j++)
		{
			printf("x[%d][%d]=",i,j);
			scanf("%d",&x[i][j]);
		}
	}
	printf ("the values are \n");
	for(i=0;i<3;i++)
	{
	for(j=0;j<3;j++)
		{
			printf("x[%d][%d]=%d \t",i,j,x[i][j]);
		}
		printf("\n");
	}
}
